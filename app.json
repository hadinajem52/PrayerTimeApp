{
  "expo": {
    "name": "مواقيت الصلاة للشيعة",
    "slug": "ShiaPrayerLeb",
    "version": "1.0.30",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "notification": {
      "icon": "./assets/favicon.png"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "This app needs access to your location to determine Qibla direction.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "This app needs access to your location to determine Qibla direction.",
        "NSCameraUsageDescription": "This app needs access to your camera for the Qibla Finder AR experience."
      }
    },
    "android": {
      "versionCode": 29,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.hnjm123.ShiaPrayerLeb",
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "CAMERA",
        "RECORD_AUDIO"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "8dbb9a8c-7250-41a6-a264-df416556a461"
      }
    },
    "owner": "hhhhjjj",
    "plugins": [
      "expo-build-properties"
    ]
  }
}
